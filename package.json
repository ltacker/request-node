{
  "name": "request-node",
  "version": "0.5.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A request-node that uses request-chain-storage instead of ethereum-storage for the storage layer.",
  "keywords": [
    "requestnetwork",
    "request-node"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "dist/server",
  "bin": {
    "request-node": "dist/server.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "build:watch": "tsc -b --watch",
    "start": "ts-node src/server.ts",
    "start:watch": "ts-node-dev src/server.ts",
    "clean": "shx rm -rf dist"
  },
  "gitHead": "6155223cfce769e48ccae480c510b35b4f54b4d0",
  "dependencies": {
    "@requestnetwork/data-access": "^0.4.2",
    "@requestnetwork/types": "^0.6.0",
    "@requestnetwork/utils": "^0.6.0",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-status-codes": "^1.3.2",
    "keyv-file": "^0.1.13",
    "request-chain-storage": "^1.0.0",
    "yargs": "^14.0.0"
  }
}
